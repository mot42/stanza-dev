<!DOCTYPE html>

<html>
  <head>
    <title>Medium General Information</title>
    {{#each css_uri}}
    <link rel="stylesheet" type="text/css" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
<div>
{{#each medium_information}}
<table class="table">
<tbody>
{{#each this}}
	<tr>
		<td>{{this.row_key}}</td>
		<td>
		{{#if this.is_array}}
		<ul>
		{{#each this.row_value}}
		<li>{{this}}</li>
		{{/each}}
		</ul>
		{{else}}{{this.row_value}}{{/if}}
		</td>
	</tr>
{{/each}}
</tbody>
</table>
{{/each}}
</div>
  </body>
</html>